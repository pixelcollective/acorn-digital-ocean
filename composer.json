{
  "name": "tiny-pixel/digital-ocean-service-provider",
  "type": "plugin",
  "license": "MIT",
  "description": "Digital Ocean Adapter for Sage 10",
  "authors": [
    {
      "name": "Kelly Mears",
      "email": "developers@tinypixel.dev",
      "homepage": "https://github.com/pixelcollective"
    }
  ],
  "autoload": {
    "psr-4": {
      "Aws\\S3\\S3Client\\": "src/",
      "League\\Flysystem\\Filesystem\\": "src/",
      "League\\Flysystem\\AwsS3v3\\AwsS3Adapter\\": "src/",
      "Roots\\Acorn\\": "src/"
    }
  },
  "require": {
    "php": "^7.1.3",
    "illuminate/support": "^5.3",
    "illuminate/filesystem": "^5.3",
    "composer/installers": "^1.0",
    "league/flysystem-aws-s3-v3": "^1.0",
    "roots/acorn": "dev-master"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.4"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "bin-dir": "./"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": [
      "phpcs"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "TinyPixel\\DigitalOceanServiceProvider"
      ]
    }
  }
}
